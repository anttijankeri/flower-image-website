{
  "name": "plant-image-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint ./src/**/*.ts",
    "pretest": "npm run lint",
    "test": "jest",
    "scss": "sass --watch src:dist",
    "dev": "vite",
    "preview": "vite preview",
    "build": "tsc && sass src:dist && vite build --base=/plant-image-website/",
    "predeploy": "npm run build",
    "deploy": "gh-pages --dist dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "gh-pages": "^6.0.0",
    "sass": "^1.69.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.5"
  }
}
